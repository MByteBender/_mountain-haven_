// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Contact {
  id String @id @default(cuid())
  name String
  email String
  message String
}



model Booking {
  id String @id @default(cuid())
  name String
  persons Int
  email String
  message String
}

model User {
  id String @id @default(cuid())
  username String
  password String
}

model Users {
  id String @id @default(cuid())
  email String @unique
  password String
}

model Admin {
  id String @id @default(cuid())
  username String @unique
  password String
}

